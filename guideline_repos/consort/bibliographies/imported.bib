@article{ref78,
  title = {{{SPIRIT}} 2025 Statement: Updated Guideline for Protocols of Randomised Trials},
  shorttitle = {{{SPIRIT}} 2025 Statement},
  author = {Chan, An-Wen and Boutron, Isabelle and Hopewell, Sally and Moher, David and Schulz, Kenneth F. and Collins, Gary S. and Tunn, Ruth and Aggarwal, Rakesh and Berkwits, Michael and Berlin, Jesse A. and Bhandari, Nita and Butcher, Nancy J. and Campbell, Marion K. and Chidebe, Runcie C. W. and Elbourne, Diana R. and Farmer, Andrew J. and Fergusson, Dean A. and Golub, Robert M. and Goodman, Steven N. and Hoffmann, Tammy C. and Ioannidis, John P. A. and Kahan, Brennan C. and Knowles, Rachel L. and Lamb, Sarah E. and Lewis, Steff and Loder, Elizabeth and Offringa, Martin and Ravaud, Philippe and Richards, Dawn P. and Rockhold, Frank W. and Schriger, David L. and Siegfried, Nandi L. and Staniszewska, Sophie and Taylor, Rod S. and Thabane, Lehana and Torgerson, David J. and Vohra, Sunita and White, Ian R. and Hr{\'o}bjartsson, Asbj{\o}rn},
  year = {2025},
